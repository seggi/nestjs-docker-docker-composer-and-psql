{
  "name": "commingle_backend",
  "version": "1.0.0",
  "description": "\"Services offer app\"",
  "main": "dist/index.js",
  "scripts": {
    "start-server": "tsc && node dist/index.js",
    "start:dev": "nodemon -e ts -w ./ -x npm run start-server",
    "build": "rimraf build/ && tsc",
    "preserve": "yarn build",
    "serve": "cross-env NODE_ENV=development concurrently \"tsc --watch\" \"nodemon -q dist/index.js\"",
    "prestart": "yarn build",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "db:create": "sequelize db:create",
    "db:create:test": "cross-env NODE_ENV=test yarn db:create",
    "db:drop": "sequelize db:drop",
    "db:drop:test": "cross-env NODE_ENV=test yarn db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:test": "cross-env NODE_ENV=test yarn db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "pretest": "cross-env NODE_ENV=test yarn migrate:reset",
    "coverage": "npm run pretest && jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "nanKim",
  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.16.0",
    "@babel/node": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/register": "^7.16.0",
    "body-parser": "^1.19.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "sequelize": "^6.12.0-alpha.1",
    "ts-node": "^10.4.0",
    "tslint": "5.12.1"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.36",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.9",
    "@types/validator": "^13.7.0",
    "concurrently": "^6.4.0",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0",
    "typescript": "^4.5.2"
  }
}